{
  "name": "@enschedule/dashboard",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "build": "remix build && cp ./build.d.ts ./build/index.d.ts",
    "dev": "remix dev",
    "build:dev": "NODE_ENV=development remix build",
    "start": "echo 'You probably want to run yarn run dev'; exit 1; remix-serve build",
    "docker:start": "NODE_ENV=production remix-serve build/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint -f visualstudio ."
  },
  "dependencies": {
    "@emotion/react": "~11.10.5",
    "@emotion/styled": "~11.10.5",
    "@enschedule/types": "workspace:*",
    "@enschedule/worker": "workspace:*",
    "@enschedule/worker-api": "workspace:*",
    "@fontsource/roboto": "~4.5.8",
    "@lexical/code": "~0.6.2",
    "@lexical/react": "~0.6.2",
    "@monaco-editor/react": "~4.4.6",
    "@mui/icons-material": "~5.10.14",
    "@mui/material": "~5.10.14",
    "@mui/utils": "~5.10.14",
    "@remix-run/node": "^2.2.0",
    "@remix-run/react": "^2.2.0",
    "@remix-run/serve": "^2.2.0",
    "@remix-run/v1-route-convention": "^0.1.4",
    "@tanstack/react-table": "~8.5.30",
    "ajv": "~8.11.2",
    "clsx": "~1.2.1",
    "cron-parser": "~4.6.0",
    "date-fns": "~2.29.3",
    "dotenv": "16.0.3",
    "immer": "^10.0.2",
    "lexical": "~0.6.2",
    "monaco-editor": "~0.34.1",
    "pascal-case": "~3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sentence-case": "~3.0.4",
    "ts-node": "^10.9.1",
    "zod": "^3.22.2",
    "zod-to-json-schema": "^3.21.4",
    "zod-to-ts": "~1.1.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.2.0",
    "@remix-run/eslint-config": "^2.2.0",
    "@types/node": "^20.5.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.0.6",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14"
  }
}
