{
  "version": 2,
  "installCommand": "NODE_ENV=development pnpm install --shamefully-hoist --force",
  "buildCommand": "./release-package.js && NODE_ENV=production turbo build",
  "builds": [
    {
      "src": "tests/hub-test/dist/index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "tests/hub-test/dist/index.js"
    }
  ]
}
